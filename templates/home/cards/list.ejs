<!-- Lists -->
<div id="lists" class="card">
  <div class="header">
    <h2>Lists</h2>
  </div>
  <div class="body">
    <p>Here is an unordered list example:</p>
    <ul>
      <li>Item 1</li>
      <li>Item 2</li>
      <li>Item 3</li>
    </ul>
    <hr>
    <p>Here is an ordered list example:</p>
    <ol>
      <li>Item 1</li>
      <li>Item 2</li>
      <li>Item 3</li>
    </ol>
    <hr>
    <p>Custom list style:</p>
    <ul>
      <li class="list-disc">Disc</li>
      <li class="list-circle">Circle</li>
      <li class="list-square">Square</li>
      <li class="list-decimal">Decimal</li>
      <li class="list-none">None</li>
    </ul>
  </div>
  <div class="footer align-right">
    <button class="btn-blue" data-open-modal="documentation-modal" data-documentation="lists">&lt;code&gt;</button>
  </div>
</div>



<div id="doc-lists" hidden>
  <h2>Lists <a href="#lists" class="no-decoration hover:underline">#</a></h2>
  <content>
    <%- include ../documentation/lists.ejs %>
  </content>
</div>